Question(QuestionText: string, PK QuestionID: guid)
QuestionTestPaper(PK QuestionTestPaperID: guid, FK QuestionID: guid, FK TestPaperID: guid)
TestPaper(PaperNumber: numeric, PK TestPaperID: guid, FK TestID: guid)
Test(Topic: string, PK TestID: guid, FK CourseID)
Course(Title: string, Description: string, PK CourseID: guid, FK InstructorID: guid)
Instructor(FirstName: string, LastName: string, PK InstructorID: guid)
Result(Score: numeric, PK ResultID: guid, FK StudentID: guid, FK TestPaperID: guid)
Student(FirstName: string, LastName: string, DateOfBirth: date, Email: string, PhoneNumber: string, PK StudentID: guid, FK GroupID: guid)
Group(Number: string, PK GroupID: guid)